{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Assitant to manage medication",
          "examplePhrases": [
            "Alexa open my medication"
          ],
          "name": "medication",
          "description": "Assitant to manage medication"
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "No special instructions.",
      "category": "EDUCATION_AND_REFERENCE",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom"
        }
      }
    },
    "manifestVersion": "1.0",
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "http://dabblelab.com/terms",
          "privacyPolicyUrl": "http://dabblelab.com/privacy"
        }
      },
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": true
    },
    "permissions": [
      {
        "name": "alexa::profile:email:read"
      },
      {
        "name": "alexa::profile:given_name:read"
      },
      {
        "name": "alexa::profile:mobile_number:read"
      },
      {
        "name": "alexa::alerts:reminders:skill:readwrite"
      }
    ]
  }
}
